{
  "name": "MorningCoffee",
  "version": "1.0.0",
  "description": "Dashboard app with tied-in apps.",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js \"npm start\"",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CatLibWilk/morning-coffee.git"
  },
  "author": "Andrew Wilk",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CatLibWilk/morning-coffee/issues"
  },
  "homepage": "https://github.com/CatLibWilk/morning-coffee#readme",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.10"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "dotenv": "^7.0.0",
    "express": "^4.17.3",
    "firebase": "^6.3.4",
    "firebase-admin": "^8.3.0",
    "pg": "^7.9.0",
    "pg-hstore": "^2.3.2",
    "react": "^16.8.6",
    "sequelize": "^5.2.8",
    "sequelize-cli": "^5.4.0"
  }
}
